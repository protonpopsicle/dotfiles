<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">
  <menu id="root-menu" label="Openbox 3">
    <separator label="Openbox" />
    <item label="st">
      <action name="Execute">
	<command>st -f "Mono-12"</command>
      </action>
    </item>
    <item label="Emacs">
      <action name="Execute">
	<command>emacs</command>
	<enabled>yes</enabled>
      </action>
    </item>
    <item label="Chromium">
      <action name="Execute">
	<command>chromium</command>
	<startupnotify>
          <enabled>yes</enabled>
	</startupnotify>
      </action>
    </item>
    <item label="Ardour">
      <action name="Execute">
	<command>ardour4</command>
	<startupnotify>
          <enabled>yes</enabled>
	</startupnotify>
      </action>
    </item>
    <item label="cmus">
      <action name="Execute">
	<command>st -f "Liberation Mono:size=12" -e cmus</command>
      </action>
    </item>
    <separator />
    <item label="ObConf">
      <action name="Execute">
	<command>obconf</command>
	<startupnotify>
	  <enabled>yes</enabled>
	</startupnotify>
      </action>
    </item>    
    <item label="Reconfigure">
      <action name="Reconfigure" />
    </item>
    <separator />
    <item label="Log Out">
      <action name="Exit">
	<prompt>yes</prompt>
      </action>
    </item>
  </menu>
  
</openbox_menu>
